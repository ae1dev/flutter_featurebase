{
  "@@locale": "tr",
  "countArticles": "<strong>{count}</strong> {count, plural, one{makale} other{makale}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "{author} tarafından yazıldı",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "lastUpdated": "Son güncelleme {daysAgo}",
  "@lastUpdated": {
    "placeholders": {
      "daysAgo": {
        "type": "String",
        "example": "28 gün"
      }
    }
  },
  "failedToLoad": "Yüklenemedi",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Daha sonra tekrar deneyin",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Sonuç bulunamadı",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "Değişiklik günlüğü",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "{appName} için yeni güncellemeler ve iyileştirmeler.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Değişiklik günlüğü bulunamadı",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Başka değişiklik günlüğü yok",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
  }
}
