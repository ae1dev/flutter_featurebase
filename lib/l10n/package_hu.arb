{
  "@@locale": "hu",
  "countArticles": "<strong>{count}</strong> {count, plural, one{cikk} other{cikk}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Írta: {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "lastUpdated": "Utoljára frissítve {daysAgo}",
  "@lastUpdated": {
    "placeholders": {
      "daysAgo": {
        "type": "String",
        "example": "28 napja"
      }
    }
  },
  "failedToLoad": "Betöltés sikertelen",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Próbálja újra később",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Nem található",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "Változásnapló",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Új frissítések és fejlesztések a(z) {appName} alkalmazásban.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Nem található változásnapló",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Nincs több változásnapló",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
  }
}
