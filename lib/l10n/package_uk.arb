{
  "@@locale": "uk",
  "countArticles": "<strong>{count}</strong> {count, plural, one{стаття} few{статті} many{статей} other{статті}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Автор: {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "lastUpdated": "Останнє оновлення {daysAgo}",
  "@lastUpdated": {
    "placeholders": {
      "daysAgo": {
        "type": "String",
        "example": "28 днів"
      }
    }
  },
  "failedToLoad": "Не вдалося завантажити",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Спробуйте пізніше",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Нічого не знайдено",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "Історія змін",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Нові оновлення та покращення {appName}.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Історію змін не знайдено",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Більше змін немає",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
  }
}
