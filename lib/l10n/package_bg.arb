{
  "@@locale": "bg",
  "countArticles": "<strong>{count}</strong> {count, plural, one{статия} other{статии}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Написано от {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "failedToLoad": "Грешка при зареждането",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Опитайте отново по-късно",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Няма резултати",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "История на промените",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Нови актуализации и подобрения в {appName}.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Няма намерени промени",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Няма повече промени",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
