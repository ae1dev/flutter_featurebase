{
  "@@locale": "id",
  "countArticles": "<strong>{count}</strong> {count, plural, one{artikel} other{artikel}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Ditulis oleh {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "lastUpdated": "Terakhir diperbarui {daysAgo}",
  "@lastUpdated": {
    "placeholders": {
      "daysAgo": {
        "type": "String",
        "example": "28 hari"
      }
    }
  },
  "failedToLoad": "Gagal memuat",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Coba lagi nanti",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Tidak ada hasil",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "Riwayat Perubahan",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Pembaruan dan peningkatan terbaru untuk {appName}.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Tidak ada riwayat perubahan",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Tidak ada riwayat perubahan lainnya",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
  }
}
