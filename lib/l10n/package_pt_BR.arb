{
  "@@locale": "pt_BR",
  "countArticles": "<strong>{count}</strong> {count, plural, one{artigo} other{artigos}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Escrito por {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "failedToLoad": "Falha ao carregar",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Tente novamente mais tarde",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Nenhum resultado encontrado",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "Histórico de alterações",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Novas atualizações e melhorias do {appName}.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Nenhum histórico encontrado",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Não há mais históricos",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
  }
}
