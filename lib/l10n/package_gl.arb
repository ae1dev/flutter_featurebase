{
  "@@locale": "gl",
  "countArticles": "<strong>{count}</strong> {count, plural, one{artigo} other{artigos}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Escrito por {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "lastUpdated": "Última actualización {daysAgo}",
  "@lastUpdated": {
    "placeholders": {
      "daysAgo": {
        "type": "String",
        "example": "28 días"
      }
    }
  },
  "failedToLoad": "Erro ao cargar",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Téntao de novo máis tarde",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Nunca se encontraron resultados",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "Rexistro de cambios",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Novas actualizacións e melloras en {appName}.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Non se atoparon rexistros de cambios",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Non hai máis rexistros de cambios",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
  }
}
