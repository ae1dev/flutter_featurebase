{
  "@@locale": "ru",
  "countArticles": "<strong>{count}</strong> {count, plural, one{статья} few{статьи} many{статей} other{статей}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Автор: {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "lastUpdated": "Последнее обновление {daysAgo}",
  "@lastUpdated": {
    "placeholders": {
      "daysAgo": {
        "type": "String",
        "example": "28 дней"
      }
    }
  },
  "failedToLoad": "Не удалось загрузить",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Повторите попытку позже",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Ничего не найдено",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "История изменений",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Новые обновления и улучшения {appName}",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "История изменений не найдена",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Больше обновлений нет",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
  }
}
