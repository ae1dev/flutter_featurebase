  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Nýjar uppfærslur og endurbætur á {appName}.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Engar breytingaskrár fundust",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
