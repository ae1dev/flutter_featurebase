{
  "@@locale": "is",
  "countArticles": "<strong>{count}</strong> {count, plural, one{grein} other{greinar}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Höfundur: {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "failedToLoad": "Ekki tókst að hlaða",
