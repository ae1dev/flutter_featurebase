{
  "@@locale": "nl",
  "countArticles": "<strong>{count}</strong> {count, plural, one{artikel} other{artikelen}}",
  "@countArticles": {
    "placeholders": {
      "count": {
        "type": "int",
        "example": "2"
      }
    }
  },
  "writtenBy": "Geschreven door {author}",
  "@writtenBy": {
    "placeholders": {
      "author": {
        "type": "String",
        "example": "John Doe"
      }
    }
  },
  "failedToLoad": "Laden mislukt",
  "@failedToLoad": {
    "description": "This is the error message that is displayed when the data fails to load."
  },
  "tryAgainLater": "Probeer het later opnieuw.",
  "@tryAgainLater": {
    "description": "This is the message that is displayed when the user should try again later."
  },
  "noResultsFound": "Geen resultaten gevonden",
  "@noResultsFound": {
    "description": "This is the message that is displayed when no results are found in search."
  },
  "changelog": "Wijzigingslogboek",
  "@changelog": {
    "description": "This is the title of the changelog page."
  },
  "changelogDescription": "Nieuwe updates en verbeteringen voor {appName}.",
  "@changelogDescription": {
    "placeholders": {
      "appName": {
        "type": "String",
        "example": "Featurebase"
      }
    }
  },
  "noChangelogsFound": "Geen wijzigingslogboeken gevonden",
  "@noChangelogsFound": {
    "description": "This is the message that is displayed when no changelogs are found."
  },
  "noMoreChangelogs": "Geen wijzigingslogboeken meer",
  "@noMoreChangelogs": {
    "description": "This is the message that is displayed when there are no more changelogs."
  }
